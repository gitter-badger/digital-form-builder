{
  "name": "digital-form-builder-designer",
  "version": "1.0.0",
  "description": "Digital forms designer (FCO)",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "fix-lint": "standard --fix",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "rollup -c -w & node index.js",
    "start": "node index.js",
    "symlink-env": "./bin/symlink-config",
    "test": "npm run lint && npm run unit && npm run unit:with-coverage && npm run check-coverage && npm audit --audit-level=high",
    "unit": "mocha --recursive test/spec/ --require test/helper --timeout 300000 --require @babel/register",
    "unit:debug": "mocha --recursive test/spec/ --require test/helper --timeout 300000 --require @babel/register --inspect-brk",
    "unit:with-coverage": "nyc mocha --recursive test/spec/ --require test/helper --timeout 300000--compilers --require @babel/register",
    "check-coverage": "nyc check-coverage --statement 90 --branch 90 --function 90 --line 90"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKForeignOffice/digital-form-builder-designer"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@hapi/hapi": "19.1.1",
    "@hapi/wreck": "17.0.0",
    "digital-form-builder-engine": "file:../engine",
    "flat": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "^7.10.1",
    "@hapi/inert": "^6.0.1",
    "@rollup/plugin-babel": "5.0.0",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-node-resolve": "7.1.3",
    "acorn": "7.2.0",
    "babel-eslint": "10.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dagre": "0.8.2",
    "dotenv": "8.2.0",
    "govuk-frontend": "3.6.0",
    "mocha": "^7.2.0",
    "nunjucks": "3.2.1",
    "nyc": "^15.1.0",
    "prismjs": "1.15.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-simple-code-editor": "0.6.1",
    "react-sortable-hoc": "0.8.3",
    "rollup": "2.9.1",
    "rollup-plugin-node-globals": "1.4.0",
    "shortid": "^2.2.15",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "standard": "12.0.1",
    "vision": "5.4.3"
  },
  "standard": {
    "ignore": [
      "dist/"
    ],
    "parser": "babel-eslint"
  }
}
